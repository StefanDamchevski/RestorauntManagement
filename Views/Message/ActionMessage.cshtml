@model RestorauntManagement.ViewModels.ActionMessage.ActionMessage

@{ 
    ViewData["Title"] = "Action Message";
}
    <div class="container">
        @if (string.IsNullOrEmpty(Model.Error))
        {
            <h3 class="alert-success" style="padding: 20px;">@Model.Message</h3>
        }
        else
        {
            <h3 class="alert-danger" style="padding: 20px;">@Model.Error</h3>
        }
    </div>
